#include <iostream>
using namespace std;
int main()
{
    int a;
    cin >> a ;
    if (a/10==9||a/10==10)
        cout << "A" ;
    else if (a/10==8)
        cout << "B";
    else if (a / 10 == 7)
        cout << "C";
    else if (a / 10 == 6)
        cout << "D";
    else
        cout << "F";
   
  
}
